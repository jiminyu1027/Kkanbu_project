  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="edu.study.mapper.QaAnswerMapper">

	<insert id="replyWrite" parameterType="edu.study.vo.QaAnswerVO">
		<![CDATA[
			INSERT INTO QUESTION_ANSWER(
				qaidx
				,QASUBJECT
				,PQIDX
				)VALUES(
				qaidx_SEQ.nextval
				,#{qaSubject}
				,#{pqidx}
				)
		]]>
	</insert>
</mapper>